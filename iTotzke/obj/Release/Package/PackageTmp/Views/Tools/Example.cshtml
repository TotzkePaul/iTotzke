@{
    ViewBag.Title = "Example";
}

<h2>Example</h2>

<style>
    .juibutton {
        height: 25px;
        width: 25px;
        display: inline-block;
    }
</style>
<script>

    $(document).ready(function () {DefaultNS.Init();

    });

    var DefaultNS = new function () {
        this.Init = function () {

        };

        this.ExampleCallback = function(nRow, aData, iDisplayIndex, iDisplayIndexFull) {

            
            var viewButton = $("<button class='juibutton'>view</button>");
            $(viewButton).button({
                icons: {
                    primary: "ui-icon-locked"
                },
                text: false
            });
        
            var editButton = $("<button class='juibutton'>edit</button>");
            $(editButton).button({
                text: false,
                icons: { primary: "ui-icon-pencil" }
            });
       

            var delButton = $("<button class='juibutton'>delete</button>").button({
                text: false,
                icons: { primary: "ui-icon-trash" }
            });

            $(".viewButton", nRow).empty().append(viewButton);
            $(".editButton", nRow).empty().append(editButton);
            $(".deleteButton", nRow).empty().append(delButton);
            return nRow;
        };

    }
</script>
<button data-button="true; icons: ui-icon-seek-end;">Button with icon only</button>
<button>Button with icon on the left</button>
<button>Button with two icons</button>
<button>Button with two icons and no text</button>

